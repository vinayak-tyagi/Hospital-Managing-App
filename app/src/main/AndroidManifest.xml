<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vinayak.app_1">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>


    <application
        android:name=".Infrastuture.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_add_icon_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_add_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER"/>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>

        <activity android:name=".Activity.HomeActivity"/>

        <activity android:name=".Activity.FeedbackActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.IPDRegisterActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.LeaveActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.MedicalStockActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.OPDRegisterActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.OtActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.OtherStockActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.ProcedureActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

        <activity android:name=".Activity.SearchActivity"
            android:parentActivityName=".Activity.HomeActivity"/>

    </application>

</manifest>